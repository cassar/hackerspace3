- content_for :title, 'Project Data Sets'
%main.team_page
  %div.main-content-header
    %h1 #{@team.name} Data Sets
  - if @team_data_sets.present?
    - @team_data_sets.each do |team_data_set|
      = render 'projects/team_data_set', locals: { data_set: team_data_set }
      = link_to 'Update Data Set', edit_team_management_team_team_data_set_path(@team, team_data_set)
      |
      = link_to 'Delete Data Set', team_management_team_team_data_set_path(@team, team_data_set), |
        method: :delete,                                                                          |
        data: { confirm: 'Are you sure?' }                                                        |
  - else
    %div Your team does not currently have any data sets.
  %aside= link_to 'Add New Data Set', new_team_management_team_team_data_set_path(@team)
%p= link_to 'Back to Team', team_management_team_path(@team)
